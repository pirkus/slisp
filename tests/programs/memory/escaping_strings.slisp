(defn make-greeting [name]
  (str "hello, " name "!"))

(defn greet-twice [name]
  (let [first (make-greeting name)
        second (make-greeting name)]
    (str "first=" first ", second=" second)))

(defn -main []
  (let [prefix (str "wor" "ld")
        suffix (str "-ok" "!")
        result (count (greet-twice (str prefix suffix)))]
    (if (= result 61) 0 1)))
